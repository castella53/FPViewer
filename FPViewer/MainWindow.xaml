        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="425" Width="500">
    <Grid x:Name="grid1" HorizontalAlignment="Left" Height="400" VerticalAlignment="Top" Width="500" ShowGridLines="True" Margin="0,0,0,-5">
        <Image x:Name="image1_1" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100"/>
        <Image x:Name="image1_2" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="100,0,0,0"/>
        <Image x:Name="image1_3" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="0,100,0,0"/>
        <Image x:Name="image1_4" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="100,100,0,0"/>
    </Grid>
</Window>
